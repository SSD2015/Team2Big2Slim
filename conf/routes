# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET         /                    controllers.Application.index()
GET         /profile             controllers.Application.profile()
POST        /creater             controllers.Application.createProject()
GET         /create              controllers.Application.addProject()

GET         /vote                controllers.Application.voting()
POST        /toVote              controllers.Application.vote(id)
GET         /voteResult          controllers.Application.getVoteResult()

GET         /group1              controllers.Application.group1()

# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file        controllers.Assets.at(path="/public", file)


    