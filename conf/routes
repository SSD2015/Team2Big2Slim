# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET         /                    controllers.Application.index()

POST        /creater             controllers.Application.createProject()
GET         /create              controllers.Application.addProject()

POST        /vote                controllers.Application.vote(String)
GET         /voting              controllers.Application.voting()

GET         /voteResult          controllers.Application.getVoteResult()

GET         /profile             controllers.Application.profile()
GET         /group1              controllers.Application.group1()

# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file        controllers.Assets.at(path="/public", file)


GET         /login               controllers.Login.login()
POST        /login               controllers.Login.authenticate()

GET         /quicknote           controllers.Quicknote.index()
POST        /quicknote           controllers.Quicknote.addNote()

GET			/rate				 controllers.Rate.rate()
POST		/rate				 controllers.Rate.submitRate()


    