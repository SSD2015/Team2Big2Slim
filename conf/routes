# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                    controllers.Application.index()

# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file        controllers.Assets.at(path="/public", file)

GET        /hello               controllers.Application.hello()

GET        /profile/:id         controllers.ProjectList.index(id:Long)

GET        /screenshot1/:id         controllers.ProjectList.sc1(id:Long)

GET        /screenshot2/:id         controllers.ProjectList.sc2(id:Long)

GET        /screenshot3/:id         controllers.ProjectList.sc3(id:Long)

GET        /test                controllers.Application.test()

GET        /install             controllers.Application.mockDatabase()
