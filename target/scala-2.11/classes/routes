# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                    controllers.Application.index()

# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file        controllers.Assets.at(path="/public", file)

GET        /hello               controllers.Application.hello()

GET        /profile/:id            controllers.ProjectList.index(id:Long)

GET        /screenshot1         controllers.Application.screenshot1()

GET        /screenshot2         controllers.Application.screenshot2()

GET        /screenshot3         controllers.Application.screenshot3()

GET        /test                controllers.Application.test()

GET        /install             controllers.Application.mockDatabase()
